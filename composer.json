{
  "name": "salvador-lopez/string-operations",
  "description": "Perform some string transformations",
  "autoload": {
    "psr-4": {
      "StringOperations\\": "src/StringOperations/",
      "Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.1",
    "symfony/framework-bundle": "^3.3",
    "symfony/console": "^3.3",
    "beberlei/assert": "^2.7",
    "symfony/yaml": "^3.3",
    "incenteev/composer-parameter-handler": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
